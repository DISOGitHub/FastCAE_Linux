        <SoftName Chinese="枪炮内弹道计算程序" name="Interior Ballistic Analysis"/>
        <Logo></Logo>
        <Welcome></Welcome>
        <Version>1.0</Version>
        <Mail>libaojunqd@foxmail.com</Mail>
        <Corporation>NUC</Corporation>
        <Describe>Memo</Describe>
        <HelpFile></HelpFile>
        <Website>http://www.nuc.edu.cn/</Website>
    </Global>
    <Material Enable="true"/>
    <Geometry>
        <ImportSuffix>stp;step;igs;iges;brep</ImportSuffix>
